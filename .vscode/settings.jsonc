{
  "[bibtex]": {
    "editor.tabSize": 2
  },
  "breadcrumbs.enabled": true,
  "cSpell.allowCompoundWords": true,
  "cSpell.language": "en-GB",
  "cSpell.maxDuplicateProblems": 2,
  "cSpell.minWordLength": 1,
  "cSpell.userWords": [
    "alright",
    "analytics",
    "biofuels",
    "blogs",
    "bokmÃ¥l",
    "celcius",
    "decarbonisation",
    "dispatchable",
    "eastings",
    "forecasted",
    "geospatial",
    "interpretivism",
    "irradiance",
    "kriging",
    "mechatronic",
    "mechatronics",
    "mentoring",
    "metrology",
    "microfluidic",
    "microfluidics",
    "nithiya",
    "nmstreethran",
    "phenomological",
    "photovoltaics",
    "prosumers",
    "renewables",
    "respirable",
    "resumÃ©",
    "stavanger",
    "streethran",
    "tribology",
    "winery",
    "zenodo",
    "zotero"
  ],
  "[css]": {
    "editor.tabSize": 2
  },
  "[csv]": {
    "editor.wordWrap": "off"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": false,
  "editor.fontFamily": "'Cascadia Code', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.minimap.renderCharacters": false,
  "editor.renderWhitespace": "all",
  "editor.rulers": [
    79
  ],
  "editor.suggestSelection": "first",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "comment",
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },
  "editor.wordWrap": "on",
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.geojson": "jsonc",
    "*.latex": "latex",
    "*.rmd": "markdown",
    "*.rst": "restructuredtext",
    "*.sty": "latex"
  },
  "files.autoSave": "afterDelay",
  "files.watcherExclude": {
    "**/env/bin/**": true,
    "**/env/lib/**": true,
    "**/env/lib64/**": true,
    "**/env/share/**": true
  },
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "gitlens.currentLine.scrollable": false,
  "[html]": {
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.tabSize": 2
  },
  "[jsonc]": {
    "editor.tabSize": 2
  },
  "jupyter.askForKernelRestart": false,
  "jupyter.exportWithOutputEnabled": true,
  "jupyter.notebookFileRoot": "${workspaceFolder}",
  "[latex]": {
    "editor.tabSize": 2
  },
  "latex-workshop.bind.altKeymap.enabled": true,
  "latex-workshop.intellisense.package.enabled": true,
  "latex-workshop.latex.autoBuild.run": "never",
  "latex-workshop.latex.autoClean.run": "onBuilt",
  "latex-workshop.latex.clean.fileTypes": [
    "*.acn",
    "*.acr",
    "*.alg",
    "*.aux",
    "*.bbl",
    "*.bcf",
    "*.blg",
    "*.fdb_latexmk",
    "*.fls",
    "*.glg",
    "*.glo",
    "*.gls",
    "*.*glg",
    "*.*glo",
    "*.*gls",
    "*.idx",
    "*.ind",
    "*.ist",
    "*.lof",
    "*.lol",
    "*.lot",
    "*.mw",
    "*.nav",
    "*.out",
    "*.run.xml",
    "*.slg",
    "*.slo",
    "*.sls",
    "*.snm",
    "*.synctex(busy)",
    "*.synctex.gz",
    "*.synctex.gz(busy)",
    "*.toc",
    "*.vrb"
  ],
  "latex-workshop.latex.recipe.default": "lastUsed",
  "latex-workshop.latex.recipes": [
    {
      "name": "arara",
      "tools": [
        "arara"
      ]
    },
    {
      "name": "latexmk ðŸ”ƒ",
      "tools": [
        "latexmk"
      ]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "arara",
      "command": "arara",
      "args": [
        "%DOC%"
      ],
      "env": {}
    }
  ],
  "latex-workshop.message.error.show": false,
  "latex-workshop.message.warning.show": false,
  "latex-workshop.view.pdf.viewer": "tab",
  "[markdown]": {
    "editor.tabSize": 2
  },
  "markdown.extension.toc.levels": "1..3",
  "material-icon-theme.activeIconPack": "none",
  "material-icon-theme.files.associations": {
    "*.geojson": "json",
    "*.latex": "tex",
    "readme.markdown": "readme",
    "readme.rst": "readme"
  },
  "material-icon-theme.folders.associations": {
    "charts": "images",
    "old": "archive",
    "wiki": "docs"
  },
  "python.dataScience.alwaysTrustNotebooks": true,
  "python.dataScience.askForKernelRestart": false,
  "python.dataScience.exportWithOutputEnabled": true,
  "python.dataScience.notebookFileRoot": "${workspaceFolder}",
  "python.linting.pylintEnabled": true,
  "python.linting.flake8Enabled": true,
  "python.terminal.activateEnvInCurrentTerminal": true,
  "[restructuredtext]": {
    "editor.tabSize": 3
  },
  "r.rpath.windows": "C:\\Program Files\\R\\R-4.0.5\\bin\\R.exe",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.fontSize": 16,
  "window.menuBarVisibility": "toggle",
  "window.restoreWindows": "none",
  "workbench.colorTheme": "GitHub Dark Default",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "workbench.iconTheme": "material-icon-theme"
}
